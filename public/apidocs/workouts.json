{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "https://cryptic-temple-10365.herokuapp.com",
  "resourcePath": "workouts",
  "apis": [
    {
      "path": "/workouts.json",
      "operations": [
        {
          "summary": "Fetches all workouts",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication-Token",
              "required": true
            }
          ],
          "notes": "There is no visibility settings at the moment; this action will allow filters and sorts.",
          "nickname": "Workouts#index",
          "method": "get"
        }
      ]
    },
    {
      "path": "/workouts.json",
      "operations": [
        {
          "summary": "Creates a workout with a given set of parameters",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication-Token",
              "required": true
            },
            {
              "paramType": "form",
              "name": "user_id",
              "type": "integer",
              "description": "User ID",
              "required": true
            },
            {
              "paramType": "form",
              "name": "title",
              "type": "integer",
              "description": "Title",
              "required": false
            },
            {
              "paramType": "form",
              "name": "time",
              "type": "string",
              "description": "Time",
              "required": true
            },
            {
              "paramType": "form",
              "name": "duration",
              "type": "string",
              "description": "Duration",
              "required": true
            },
            {
              "paramType": "form",
              "name": "location",
              "type": "string",
              "description": "Location",
              "required": true,
              "allowableValues": {
                "valueType": "LIST",
                "values": [
                  "WIEGAND GYMNASIUM",
                  "JARED L. COHON CENTER GYMNASIUM",
                  "SKIBO GYMNASIUM"
                ]
              }
            },
            {
              "paramType": "form",
              "name": "team_size",
              "type": "string",
              "description": "Team Size",
              "required": true
            }
          ],
          "notes": "For now, we are passing the owner as a parameter as we do not have authentication",
          "responseMessages": [
            {
              "code": 406,
              "responseModel": null,
              "message": "Not Acceptable"
            }
          ],
          "nickname": "Workouts#create",
          "method": "post"
        }
      ]
    },
    {
      "path": "/workouts/{id}.json",
      "operations": [
        {
          "summary": "Shows a particular workout's data",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication-Token",
              "required": true
            },
            {
              "paramType": "path",
              "name": "id",
              "type": "integer",
              "description": "Workout ID",
              "required": true
            }
          ],
          "notes": "This fetches all data related to the workout, including owner and joined workouts.",
          "responseMessages": [
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            }
          ],
          "nickname": "Workouts#show",
          "method": "get"
        }
      ]
    },
