{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:3000",
  "resourcePath": "users",
  "apis": [
    {
      "path": "/users.json",
      "operations": [
        {
          "summary": "Fetches all users",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication-Token",
              "required": true
            }
          ],
          "notes": "There is a lot of authentication at the moment",
          "nickname": "Users#index",
          "method": "get"
        }
      ]
    },
    {
      "path": "/users.json",
      "operations": [
        {
          "summary": "Creates a user with a given set of parameters",
          "parameters": [
            {
              "paramType": "form",
              "name": "facebook_id",
              "type": "integer",
              "description": "Facebook ID",
              "required": false
            },
            {
              "paramType": "form",
              "name": "email",
              "type": "string",
              "description": "Email",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password",
              "type": "string",
              "description": "Password",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password_confirmation",
              "type": "string",
              "description": "Password Confirmation",
              "required": true
            },
            {
              "paramType": "form",
              "name": "role",
              "type": "string",
              "description": "Role",
              "required": true,
              "allowableValues": {
                "valueType": "LIST",
                "values": [
                  "Real"
                ]
              }
            },
            {
              "paramType": "form",
              "name": "first_name",
              "type": "string",
              "description": "First Name",
              "required": true
            },
            {
              "paramType": "form",
              "name": "last_name",
              "type": "string",
              "description": "Last Name",
              "required": true
            },
            {
              "paramType": "form",
              "name": "gender",
              "type": "string",
              "description": "Gender",
              "required": true,
              "allowableValues": {
                "valueType": "LIST",
                "values": [
                  "Male",
                  "Female",
                  "Other"
                ]
              }
            },
            {
              "paramType": "form",
              "name": "age",
              "type": "integer",
              "description": "Age",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 406,
              "responseModel": null,
              "message": "Not Acceptable"
            }
          ],
          "nickname": "Users#create",
          "method": "post"
        }
      ]
    },
    {
      "path": "/users/{id}.json",
      "operations": [
        {
          "summary": "Shows a particular user's data",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication-Token",
              "required": true
            },
            {
              "paramType": "path",
              "name": "id",
              "type": "integer",
              "description": "User ID",
              "required": true
            }
          ],
          "notes": "This fetches a single user",
          "responseMessages": [
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            }
          ],
          "nickname": "Users#show",
          "method": "get"
        }
      ]
    },
    {
      "path": "/users/{id}.json",
      "operations": [
        {
          "summary": "Updates a user with a given set of parameters",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication-Token",
              "required": true
            },
            {
              "paramType": "path",
              "name": "id",
              "type": "integer",
              "description": "User ID",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password",
              "type": "string",
              "description": "Password",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password_confirmation",
              "type": "string",
              "description": "Password Confirmation",
              "required": true
            },
            {
              "paramType": "form",
              "name": "first_name",
              "type": "string",
              "description": "First Name",
              "required": true
            },
            {
              "paramType": "form",
              "name": "last_name",
              "type": "string",
              "description": "Last Name",
              "required": true
            },
            {
              "paramType": "form",
              "name": "gender",
              "type": "string",
              "description": "Gender",
              "required": true
            },
            {
              "paramType": "form",
              "name": "age",
              "type": "integer",
              "description": "Age",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            },
            {
              "code": 406,
              "responseModel": null,
              "message": "Not Acceptable"
            }
          ],
          "nickname": "Users#update",
          "method": "patch"
        }
      ]
    },
    {
      "path": "/users/{id}.json",
      "operations": [
        {
          "summary": "Destroys a user with a given set of parameters",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication-Token",
              "required": true
            },
            {
              "paramType": "path",
              "name": "id",
              "type": "integer",
              "description": "User ID",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 404,
              "responseModel": null,
              "message": "Not Found"
            }
          ],
          "nickname": "Users#destroy",
          "method": "delete"
        }
      ]
    }
  ],
  "authorizations": null
}